
SET statement_timeout = 0;
SET lock_timeout = 0;
SET idle_in_transaction_session_timeout = 0;
SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;
SELECT pg_catalog.set_config('search_path', '', false);
SET check_function_bodies = false;
SET xmloption = content;
SET client_min_messages = warning;
SET row_security = off;


CREATE SCHEMA healspan1;


ALTER SCHEMA healspan1 OWNER TO root;



CREATE FUNCTION healspan1.check_claim_stage_link_id() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
 declare v_RECCONT INTEGER;
begin
   select COUNT(*) INTO v_RECCONT from claim_assignment where claim_stage_link_id = NEW.claim_stage_link_id;

	if v_RECCONT < 1 THEN
	 	  RETURN NEW;
	END IF;

END;
$$;


ALTER FUNCTION healspan1.check_claim_stage_link_id() OWNER TO root;



CREATE FUNCTION healspan1.function_1(refcursor) RETURNS refcursor
    LANGUAGE plpgsql
    AS $_$
BEGIN
        OPEN $1 FOR SELECT * FROM healspan1.patient_info;
        RETURN $1;
END;
$_$;


ALTER FUNCTION healspan1.function_1(refcursor) OWNER TO root;



CREATE PROCEDURE healspan1.get_patient_info(IN _id integer, INOUT _result_one refcursor DEFAULT 'result'::refcursor)
    LANGUAGE plpgsql
    AS $$
BEGIN


  open _result_one for
    SELECT * from patient_info where id = _ID    ;



END;
$$;


ALTER PROCEDURE healspan1.get_patient_info(IN _id integer, INOUT _result_one refcursor) OWNER TO root;



